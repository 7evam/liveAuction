    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-transform-es2015-spread": "^6.22.0",
    "clean-webpack-plugin": "^0.1.17",
    "css-loader": "^1.0.1",
