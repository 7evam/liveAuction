  "scripts": {
    "dev": "concurrently 'nodemon server.js --hot' 'webpack-dev-server --hot --mode=development'",
    "db:init": "dropdb --if-exists live_auction && createdb live_auction",
    "db:reset": "./scripts/resetDB",
    "db:seed": "./scripts/seedDB",
    "build:dev": "webpack -d --progress --mode=development",
    "build:watch": "webpack-dev-server --hot --mode=development",
    "build": "webpack -p --mode=production",
    "clean": "rimraf node_modules dist public yarn.lock"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "concurrently": "^4.0.1",
    "csv-parse": "^3.1.3",
    "express": "^4.16.4",
