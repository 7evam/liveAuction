    "pg": "^7.6.0",
    "sequelize": "^4.41.0",
    "socket.io": "^2.1.1"
  },
  "webpack_build": {
    "entry": "./src/index.js",
    "output": "public",
    "title": "Live Auction",
    "react_mount_root": "root"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
